//Write a c program to check whether 2 numbers are amicable or not.
#include<stdio.h>
int main()
{
int a,b,i;
int c=0;
int d=0;
printf("Enter two numbers to check if Amicable or not : ");
scanf("%d %d",&a,&b);
for(i=1;i<a;i++)
{
if(a % i==0)
{
c+=i;
}
}
for(int j=1;j<b;j++)
{
if(b % i== 0)
{
d+=i;
}
}
if((a==d) && (b==c))
{
printf("%d and %d are Amicable numbers\n",a,b);
    }
else
{
printf("%d and %d are not Amicable numbers\n",a,b);
}
}
